{
    "name": "GoodData Developer Extension",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Various helpers for GoodData client development.",
    "background": {
    	"scripts": ["background.js"]
    },
    "content_scripts": [{
        "matches": ["*"],
        "js": "content_script.js"
    }],
    "permissions": ["contextMenus", "tabs"],
    "icons": {
        "16": "gooddata16.ico"
    }
}
